Program.Sub.Check_Custom_Tables.Start
F.Intrinsic.Control.SetErrorHandler("Check_Custom_Tables_Err")
F.Intrinsic.Control.ClearErrors
v.Local..BulkDeclareString(sError,sSQL,sNum_Type_Pairs,sNum_Type,sText)
v.Local.bExist.Declare
v.Local.iCnt.Declare

'BDataType
f.Intrinsic.String.Split("0*CHAR,1*INT,2*DFR,3*DATE,4*TIME,5*DECIMAL,6*MONEY,7*BIT,8*NUMERIC,9*BFLOAT,11*VARCHAR,14*UINT,15*IDENTITY,16*BIT,17*NUMERICSTS,18*NUMERICSA,19*CURRENCY,20*TIMESTAMP,21*LONGVARCHAR,25*NCHAR,26*NVARCHAR,28*NUMERICSLB,29*NUMERICSLS,30*DATETIME,31*NUMERICSTB",",",v.Local.sNum_Type_Pairs)

Function.ODBC.Connection!conx.TableExists("BDataType",v.Local.bExist)
f.Intrinsic.Control.If(v.Local.bExist,=,false)
	f.Intrinsic.UI.InvokeWaitDialog("Creating data type values...")
	f.ODBC.Connection!conx.Execute("CREATE TABLE BDataType(DataTypeNumber INTEGER,DataTypeName CHAR(15));")
	f.ODBC.Connection!conx.Execute("CREATE INDEX DTN ON BDataType(DataTypeNumber);")
	f.Intrinsic.Control.For(v.Local.iCnt,0,v.Local.sNum_Type_Pairs.UBound,1)
		f.Intrinsic.String.Split(v.Local.sNum_Type_Pairs(v.Local.iCnt),"*",v.Local.sNum_Type)
		f.Intrinsic.String.Build("Insert into BDataType values({0},'{1}')",v.Local.sNum_Type(0),v.Local.sNum_Type(1),v.Local.sSQL)
		f.ODBC.Connection!conx.Execute(v.Local.sSQL)
	F.Intrinsic.Control.Next(V.Local.iCnt)
	f.Intrinsic.UI.CloseWaitDialog
f.Intrinsic.Control.Endif

'B_Field_Notes
v.Local.bExist.Set(false)
Function.ODBC.Connection!conx.TableExists("B_Field_Notes",v.Local.bExist)
f.Intrinsic.Control.If(v.Local.bExist,=,false)
	f.Intrinsic.UI.InvokeWaitDialog("Creating B_Field_Notes tables...")	
	f.ODBC.Connection!conx.Execute("CREATE TABLE B_FIELD_NOTES(TABLENAME CHAR(20),FIELDNAME CHAR(20),NOTE CHAR(50));")
	f.ODBC.Connection!conx.Execute("CREATE INDEX TF ON B_FIELD_NOTES(TableName,FieldName);")
	f.Intrinsic.UI.CloseWaitDialog
f.Intrinsic.Control.Endif

f.Intrinsic.Control.ExitSub

F.Intrinsic.Control.Label("Check_Custom_Tables_Err")
F.Intrinsic.Control.If(V.Ambient.ErrorNumber,<>,0)
	Function.Intrinsic.String.Concat("Project: FYFs_0_1.g2u",V.Ambient.Newline,V.Ambient.Newline,"Subroutine: ",V.Ambient.CurrentSubroutine,V.Ambient.NewLine,"Error Occurred ",V.Ambient.ErrorNumber," with description ",V.Ambient.ErrorDescription,V.Local.sError)
