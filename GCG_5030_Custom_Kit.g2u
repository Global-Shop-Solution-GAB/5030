Program.Sub.gs_CellValueChanged.Start
F.Intrinsic.Control.SetErrorHandler("gs_CellValueChanged_Err")
F.Intrinsic.Control.ClearErrors
V.Local..BulkDeclareString(sError,sSQL,sRowNumber,sNote)
v.Local..BulkDeclareLong(iRowNumber,iExist)

Gui.Form.gs.GetSelectedRows("GV",v.Local.sRowNumber)
v.Local.iRowNumber.Set(v.Local.sRowNumber)
V.Local.sNote.Set(v.DataTable.DT(v.Local.iRowNumber).Note!FieldVAlTrim)

F.Intrinsic.Control.If(V.Local.sNote.Length,>,30)
	f.Intrinsic.UI.Msgbox("Note must have 30 characters max")
	'Put back original Note to the cell
	f.Intrinsic.String.Build("Select Note from B_Field_Notes where TableName = '{0}' and FieldName = '{1}'",v.DataTable.DT(v.Local.iRowNumber).TableName!FieldVAlString,v.DataTable.DT(v.Local.iRowNumber).FieldName!FieldVAlString,v.Local.sSQL)
	f.ODBC.Connection!conx.ExecuteAndReturn(v.Local.sSQL,v.Local.sNote)
	f.Data.DataTable.SetValue("DT",v.Local.iRowNumber,"Note",v.Local.sNote)
F.Intrinsic.Control.Else
	f.Intrinsic.String.Build("Select count(*) from B_Field_Notes where TableName = '{0}' and FieldName = '{1}'",v.DataTable.DT(v.Local.iRowNumber).TableName!FieldVAlString,v.DataTable.DT(v.Local.iRowNumber).FieldName!FieldVAlString,v.Local.sSQL)
	f.ODBC.Connection!conx.ExecuteAndReturn(v.Local.sSQL,v.Local.iExist)
	f.Intrinsic.Control.If(v.Local.iExist,>,0)
		f.Intrinsic.String.Build("Update B_Field_Notes Set Note = '{0}' where TableName = '{1}' and FieldName = '{2}'",v.Local.sNote,v.DataTable.DT(v.Local.iRowNumber).TableName!FieldVAlString,v.DataTable.DT(v.Local.iRowNumber).FieldName!FieldVAlString,v.Local.sSQL)
	f.Intrinsic.Control.Else
		f.Intrinsic.String.Build("Insert into B_Field_Notes values('{0}','{1}','{2}')",v.DataTable.DT(v.Local.iRowNumber).TableName!FieldVAlString,v.DataTable.DT(v.Local.iRowNumber).FieldName!FieldVAlString,v.Local.sNote,v.Local.sSQL)
	f.Intrinsic.Control.EndIf
	F.ODBC.Connection!conx.Execute(V.Local.sSQL)
F.Intrinsic.Control.EndIf

F.Intrinsic.Control.ExitSub

F.Intrinsic.Control.Label("gs_CellValueChanged_Err")
F.Intrinsic.Control.If(V.Ambient.ErrorNumber,<>,0)
	Function.Intrinsic.String.Concat("Project: FindFields.g2u",V.Ambient.Newline,V.Ambient.Newline,"Subroutine: ",V.Ambient.CurrentSubroutine,V.Ambient.NewLine,"Error Occurred ",V.Ambient.ErrorNumber," with description ",V.Ambient.ErrorDescription,V.Local.sError)
